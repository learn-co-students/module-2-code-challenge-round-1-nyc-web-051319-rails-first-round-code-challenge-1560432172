<h2>All Heroines</h2>


<h2>Search by SuperPower</h2>
<%=form_tag "/heroines/results" , :method => 'get' do %>
<%= text_field_tag  :query %>
<%= submit_tag 'Submit' %>
<%end %>

<ul>
  <% @heroines.each do |heroine| %>
    <li>
      <%=link_to heroine.super_name, heroine_path(heroine) %>
    </li>
    <br>
  <% end %>
</ul>
