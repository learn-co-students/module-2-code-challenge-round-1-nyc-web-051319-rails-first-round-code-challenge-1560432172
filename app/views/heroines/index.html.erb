<h2>All Heroines</h2>

<div>
  <p>Search by Superpower:</p>
  <%=form_tag heroines_path, method: "get" do%>
  <%=text_field_tag :search%>
  <%=submit_tag "Search"%>
  <%end%>
</div>

<ul>
  <% @heroines.each do |heroine| %>
    <li>
      <%= link_to heroine.super_name, heroine %>
    </li>
    <br>
  <% end %>
</ul>

<%=button_to "New Heroine", new_heroine_path, method: "get"%>
