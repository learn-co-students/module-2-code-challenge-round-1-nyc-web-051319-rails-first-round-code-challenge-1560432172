<h2>All Heroines</h2>

<%= form_tag("/heroines", method: "get") do %>
  <%= label_tag(:q, "Search by Power:") %>
  <%= text_field_tag(:q) %>
  <%= submit_tag("Search") %>
<% end %>

<ul>
  <% @heroines.each do |heroine| %>
    <li>
      <%=link_to "#{heroine.super_name}",heroine_path(heroine) %>
    </li>
    <br>
  <% end %>
</ul>
<%=button_to "Power's Page", '/powers', :method => :get%>
<%=button_to "New Heroine", {:action => "new"}, :method => :get%>
