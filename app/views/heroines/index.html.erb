<h2>All Heroines</h2>
<h4>Search by Superpower</h4>
 <%=form_tag(heroines_path, method: :get) do  %>
 <%= text_field_tag(:search, params[:search]) %>
 <%= submit_tag ("Search") %>
 <% end %>

<ul>
  <% @heroines.each do |heroine| %>
    <li>
      <%=link_to heroine.super_name, "/heroines/#{heroine.id}" %>
    </li>
    <br>
  <% end %>
</ul>
<%= button_to('Add Heroine', "/heroines/new", method: "get") %>
