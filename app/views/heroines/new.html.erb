<h1>Create a New Heroine</h1>
<%if flash[:message] %>
<%= message %>
<% end %>

<%= form_for @heroine do |f| %>
<%= f.label :name, "Name" %>
<%= f.text_field :name %>
<%= f.label :super_name, "Super Name" %>
<%= f.text_field :super_name %>
<%= f.label :power_id, "Power ID" %>
<%= f.collection_select :power_id, @powers, :id, :name, prompt: true %>
<%= f.submit "Submit"%>
<% end %>