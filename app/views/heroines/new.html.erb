<% if flash[:notice] %>
<div class="notice"><%= flash[:notice] %></div>
<% end %>
<%=form_for @heroine do |f|%>
<%=f.label "Name"%>
<%=f.text_field :name%>
<%=f.label "Super Name"%>
<%=f.text_field :super_name%>
<%=f.collection_select(:power_id, Power.all, :id, :name)%>
<%=f.submit "Submit"%>
<%end%>

