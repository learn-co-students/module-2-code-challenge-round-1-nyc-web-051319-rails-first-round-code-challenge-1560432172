<h1>Create a New Super Heroine!</h1>
<hr>
<%= form_for @heroine do |f| %>
<%= f.label :name, "Real name:" %>
<%= f.text_field :name %>
<%= f.label :super_name, "Super name:" %>
<%= f.text_field :super_name %>
<%= f.label :power_id, "Super Power:" %>
<%= f.collection_select :power_id, @powers, :id, :name, prompt: true %>
<%= f.submit %>
<% end %>